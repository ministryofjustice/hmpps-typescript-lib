{
  "name": "@ministryofjustice/eslint-config-hmpps",
  "version": "0.0.1-beta.2",
  "description": "ESLint rules for HMPPS typescript projects",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "hmpps"
  ],
  "author": "hmpps-developers",
